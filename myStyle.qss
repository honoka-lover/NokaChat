QFrame#mainUI{
    border-image:url(:/source/png/transparent.png);
}

/*左侧栏UI样式*/
#listWidget{
    background:transparent;
     border-style:flat;
}


QListWidget#listWidget::item{
    color:white;
    border:none;
    border-style:flat;
}

QListWidget#listWidget::item:selected{
    color:white;
    background-color: rgba(234, 140, 103, 0.5);
    border:none;
    border-style:flat;
}

QFrame{
    border-style:flat;
}


